    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch Generator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/generator",
            "args": ["10", "500", "dates.txt"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "build"
        },
        {
            "name": "(lldb) Launch App",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/app",
            "args": ["dates.txt"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "build"
        }

    ]
}